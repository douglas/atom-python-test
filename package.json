{
  "name": "python-test",
  "author": "Moxley Stratton / Douglas Soares de Andrade",
  "homepage": "https://github.com/moxley/atom-python-test",
  "bugs": "https://github.com/moxley/atom-python-test/issues",
  "main": "./lib/python-test",
  "version": "1.0.2",
  "description": "Run Python tests from Atom",
  "activationCommands": {
    "atom-workspace": [
      "python-test:toggle",
      "python-test:test-file",
      "python-test:test-single",
      "python-test:test-previous",
      "python-test:test-all"
    ]
  },
  "repository": "https://github.com/douglas/atom-python-test",
  "license": "MIT",
  "engines": {
    "atom": ">=1.2.0 <2.0.0"
  },
  "dependencies": {
    "ansi-to-html": "^0.2",
    "atom-space-pen-views": "^2.0.3",
    "atom-package-deps": "^4.3"
  },
  "package-deps": [
    "platformio-ide-terminal"
  ],
  "keywords": [
    "python",
    "test",
    "pytest",
    "py.test",
    "unittest"
  ],
  "consumedServices": {
    "runInTerminal": {
      "versions": {
        "0.14.x": "consumeRunInTerminal"
      }
    }
  }
}
